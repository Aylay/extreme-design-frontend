		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"luxon": "^3.4.4",
		"postcss": "^8.4.22",
		"prettier": "^2.8.0",
